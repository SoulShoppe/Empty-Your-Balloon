{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3.1"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "mkdir -p static_cache; node ./bin/www",
    "start-production": "mkdir -p static_cache; npm install; bower install --allow-root; node ./bin/www-production"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "sqlite3": "~3.1.0",
    "node-uuid": "~1.4.3",
    "express-minify": "~0.1.6",
    "node-sass": "~3.3.3",
    "compression": "~1.6.0",
    "respawn": "~2.0.0",
    "simple-node-logger": "~0.92.21",
    "node-sass-middleware": "~0.9.6"
  }
}
